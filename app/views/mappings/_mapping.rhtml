    <td><%= link_to "#{h mapping.source.manufacturer} #{h mapping.source.name}", 
                    edit_product_path(mapping.source) %></td>
    <td><%= link_to "#{h mapping.destination.manufacturer} #{h mapping.destination.name}", 
                    edit_product_path(mapping.destination) %></td>
