<h2>New mapping</h2>

<%= error_messages_for :mapping %>

<% form_for(:mapping, :url => mappings_path) do |f| %>
  <p>
    <b>Source</b><br />
    <%= f.text_field :source_id %>
  </p>

  <p>
    <b>Dest</b><br />
    <%= f.text_field :destination_id %>
  </p>

  <p>
    <b>Migration cost</b><br />
    <%= f.text_field :migration_cost %>
  </p>

  <p>
    <%= submit_tag "Create" %>
  </p>
<% end %>

<%= link_to 'Back', mappings_path %>