<h2>Listing Components</h2>

<table class="sp">
  <tr>
    <th>Manufacturer</th>
    <th>Product</th>
    <th>Type</th>
    <th></th>
    <th></th>
  </tr>
  
<% for component in @components %>
  <tr>
    <td><%=h component.product.manufacturer %></td>
    <td><%=h component.product.name %></td>
    <td><%=h component.product.class.name %></td>
  </tr>
<% end %>
</table>

<br />
